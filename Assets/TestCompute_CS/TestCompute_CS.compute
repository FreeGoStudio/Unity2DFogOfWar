#pragma kernel InitBackground
#pragma kernel CSMain
RWTexture2D<float4> Result;

[numthreads(8,8,1)]
void InitBackground(uint3 id : SV_DispatchThreadID)
{
    uint2 pixel = id.xy;
    Result[pixel] = float4(1, 0, 0, 1);
}

void CSMain(uint3 id : SV_DispatchThreadID)
{
    uint2 pixel = id.xy;
    Result[pixel] = float4(1, 0, 0, 1);
}